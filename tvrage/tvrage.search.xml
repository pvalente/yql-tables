<?xml version="1.0" encoding="UTF-8"?>
<table xmlns="http://query.yahooapis.com/v1/schema/table.xsd">
    <meta>
	    <author>Micah Laaker (micah@laaker.com)</author>
	    <description>TVRage Search</description>
        <documentationURL>http://services.tvrage.com/info.php?page=main</documentationURL>
	    <sampleQuery>select * from {table} where show='mad men';</sampleQuery>
    </meta>
    <bindings>
        <select itemPath="" produces="XML">
            <urls>
                <url>http://services.tvrage.com/feeds/search.php</url>
            </urls>
            <inputs>
                <key id="show" type="xs:string" paramType="query" required="true"/>
            </inputs>
        </select>
     </bindings>
</table>